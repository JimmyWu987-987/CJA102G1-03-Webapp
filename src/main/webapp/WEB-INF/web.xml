<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>CJA102G1ProReport</display-name>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/myproject</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <servlet-name>ProReportServlet</servlet-name>
    <servlet-class>com.proreport.controller.ProReportServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProReportServlet</servlet-name>
    <url-pattern>/proreport/proreport.do</url-pattern>
  </servlet-mapping>
</web-app>